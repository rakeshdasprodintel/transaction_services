{
  "name": "transaction_services",
  "version": "1.0.0",
  "description": "Assignment of transaction_services micro-services in node!",
  "author": "Rakesh Kumar Das <lucky2045@gmail.com>",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "scripts": {
    "start": "node index.js",
    "start:dev": "./node_modules/nodemon/bin/nodemon.js start index.js",
    "start:debug": "cross-env DEBUG=tradila:* yarn start",
    "lint": "esw *.js server config --color",
    "lint:watch": "yarn lint -- --watch",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha --ui bdd --reporter spec --colors server --recursive",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "cross-env NODE_ENV=test ./node_modules/.bin/istanbul cover _mocha -- --ui bdd --reporter spec --colors server --recursive",
    "test:check-coverage": "yarn test:coverage && istanbul check-coverage",
    "report-coverage": "coveralls < ./coverage/lcov.info"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "express",
    "node",
    "node.js",
    "mongodb",
    "mongoose",
    "es6",
    "mocha",
    "istanbul",
    "REST",
    "API",
    "boilerplate"
  ],
  "dependencies": {
    "@sentry/node": "^5.11.0",
    "app-root-path": "2.2.1",
    "await-to-js": "2.1.1",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "cookie-parser": "1.4.3",
    "cors": "2.8.4",
    "debug": "^2.4.5",
    "dotenv": "^4.0.0",
    "express": "4.17.3",
    "express-fileupload": "^1.1.6",
    "express-jwt": "5.3.1",
    "express-validation": "1.0.2",
    "express-winston": "2.5.0",
    "fcm-node": "^1.5.2",
    "fcm-push": "^1.1.3",
    "fs": "0.0.1-security",
    "helmet": "3.12.0",
    "http-status": "1.0.1",
    "https": "^1.0.0",
    "joi": "10.6.0",
    "jsonwebtoken": "7.1.9",
    "method-override": "^2.3.10",
    "mysql2": "^1.7.0",
    "nodemailer": "^6.4.2",
    "package.json": "^2.0.1",
